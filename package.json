{
  "name": "webflow-markdown",
  "version": "1.0.0",
  "author": "Christian Ekrem <christian@enso.no>",
  "license": "MIT",
  "scripts": {
    "postinstall": "elm-tooling install",
    "build": "elm make src/Main.elm --output=public_html/elm.js --optimize",
    "dev": "elm-live src/Main.elm --dir=./public_html -- --output=public_html/elm.js --debug",
    "start": "http-server public_html --port=$PORT"
  },
  "devDependencies": {
    "elm-live": "^4.0.2",
    "elm-tooling": "^1.15.1",
    "prettier": "^3.5.3",
    "prettier-plugin-elm": "^0.11.0",
    "prettier-plugin-tailwindcss": "^0.6.11"
  },
  "dependencies": {
    "http-server": "^14.1.1"
  }
}
